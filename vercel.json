{
  "version": 2,
  "name": "orbital-field-station",
  "builds": [
    {
      "src": "OrbitalPort.html",
      "use": "@vercel/static"
    },
    {
      "src": "robots.txt",
      "use": "@vercel/static"
    },
    {
      "src": "sitemap.xml",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/OrbitalPort.html"
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/sitemap.xml"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noai, noimageai, noarchive, nosnippet"
        },
        {
          "key": "X-Copyright",
          "value": "All content protected - No AI training permitted"
        },
        {
          "key": "X-License",
          "value": "All rights reserved - Contact signal@jackdanger.dev for permissions"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "no-referrer-when-downgrade"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self' 'unsafe-inline' https://cdn.tailwindcss.com https://fonts.googleapis.com https://fonts.gstatic.com; img-src 'self' data:; frame-ancestors 'none';"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/index.html",
      "destination": "/OrbitalPort.html"
    }
  ],
  "functions": {
    "OrbitalPort.html": {
      "maxDuration": 10
    }
  },
  "regions": ["iad1"],
  "github": {
    "silent": true
  }
}